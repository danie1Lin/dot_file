{
  "list.source.gfiles.defaultOptions": ["--number-select"],
  "solargraph.diagnostics": true,
  "list.normalMappings": {
    "v": "action:vsplit",
    "s": "action:split"
  },
  "solargraph.formatting": true,
  "solargraph.logLevel": "debug",
  "solargraph.trace.server": "verbose",
  "languageserver": {
    "terraform": {
      "command": "terraform-lsp",
      "filetypes": ["terraform", "tf"],
      "initializationOptions": {}
    },
    "puppet": {
      "command": "ruby",
      "args": [
        "/Users/daniellin/puppet-editor-services/puppet-languageserver",
        "--stdio",
        "--puppet-settings=--moduledir,./modules"
      ],
      "filetypes": ["puppet"],
      "rootPatterns": [".git/", "metadata.json"]
    }
  },
  "browser.patterns": {
    "*": []
  },
  "browser.port": 9999,
  "python.pythonPath": "python",
  "list.source.grep.command": "ag",
  "git.addedSign.hlGroup": "GitGutterAdd",
  "git.changedSign.hlGroup": "GitGutterChange",
  "git.removedSign.hlGroup": "GitGutterDelete",
  "git.topRemovedSign.hlGroup": "GitGutterDelete",
  "git.changeRemovedSign.hlGroup": "GitGutterChangeDelete",
  "git.gitlab.hosts": ["gitlab.kkinternal.com"],
  "coc.preferences.jumpCommand": "CocSplitIfNotOpen",
  "coc.preferences.formatOnSaveFiletypes": [
    "ruby",
    "json",
    "go",
    "rust",
    "python"
  ],
  "cSpell.userWords": [
    "asyncio",
    "deque",
    "newebpay",
    "reqwest",
    "reqwst",
    "tmpl"
  ],
  "go.goplsPath": "~/go/bin/gopls",
  "go.goplsUseDaemon": false,
  "go.tags": {
    "transform": "camelcase",
    "options": ""
  }
}
